-- =============================================
-- =============================================
CREATE PROCEDURE [dbo].[GIBS_MealTracker_GetLocationByID] 
	-- Add the parameters for the stored procedure here

@LocationID int
AS
BEGIN

SELECT [LocationID]
      ,[PortalID]
      ,[LocationCode]
      ,[Location]
      ,[Address]
      ,[City]
      ,[Region]
      ,[ZipCode]
      ,[DESE_Breakfast]
      ,[DESE_Lunch]
      ,[DESE_Snack]
	  ,[DESE_Snack_PM]
      ,[IsActive]
      ,[CreatedOnDate]
      ,[CreatedByUserID]
      ,[LastModifiedByUserID]
      ,[LastModifiedOnDate]
  FROM [dbo].[GIBS_MealTrackerLocations]
where (1 = 1)
AND (LocationID = @LocationID)


END
GO

